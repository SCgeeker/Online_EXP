}

@article{Gnambs-2023,
  title = {Remote {{Testing}} of {{Reading Comprehension}} in 8-{{Year-Old Children}}: {{Mode}} and {{Setting Effects}}},
  shorttitle = {Remote {{Testing}} of {{Reading Comprehension}} in 8-{{Year-Old Children}}},
  author = {Gnambs, Timo and Lenhard, Wolfgang},
  date = {2023-03-08},
  journaltitle = {Assessment},
  shortjournal = {Assessment},
  pages = {107319112311593},
  issn = {1073-1911, 1552-3489},
  doi = {10.1177/10731911231159369},
  url = {http://journals.sagepub.com/doi/10.1177/10731911231159369},
  urldate = {2023-03-14},
  abstract = {Proctored remote testing of cognitive abilities in the private homes of test-takers is becoming an increasingly popular alternative to standard psychological assessments in test centers or classrooms. Because these tests are administered under less standardized conditions, differences in computer devices or situational contexts might contribute to measurement biases that impede fair comparisons between test-takers. Because it is unclear whether cognitive remote testing might be a feasible assessment approach for young children, the present study ( N = 1,590) evaluated a test of reading comprehension administered to children at the age of 8 years. To disentangle mode from setting effects, the children finished the test either in the classroom on paper or computer or remotely on tablets or laptops. Analyses of differential response functioning found notable differences between assessment conditions for selected items. However, biases in test scores were largely negligible. Only for children with below-average reading comprehension small setting effects between on-site and remote testing were observed. Moreover, response effort was higher in the three computerized test versions, among which, reading on tablets most strongly resembled the paper condition. Overall, these results suggest that, on average, even for young children remote testing introduces little measurement bias.},
  langid = {english},
  file = {D:\core\reading\unsort\10731911231159369.pdf}
}

